/* genero igual */
s.t. Q114_genero{p1 in PACIENTE, p2 in PACIENTE}:
	(Q114[p1]+Q114[p2])+(abs(genero[p1]-genero[p2])/2) <= 2;



/***************************************************
* Quarto 105
* Leitos:	105a	105b	
***************************************************/
var Q105{p in PACIENTE} binary;
/* max 2 leitos */
s.t. Q105_max: sum {p in PACIENTE} Q105[p] <= 2;



/***************************************************
